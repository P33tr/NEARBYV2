@page "/"
@inject ISnackbar Snackbar
@inherits IndexBase
<LeafletMap Map="PositionMap" TileLayer="OpenStreetMapsTileLayer" />
@code
{
    protected override async Task OnInitializedAsync()
    {
        await Task.Delay(1);
        Snackbar.Add(" <MudAlert Severity='Severity.Info'>We are in the main page</MudAlert>", severity:Severity.Success);
    }
}
